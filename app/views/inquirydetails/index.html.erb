<p id="notice"><%= notice %></p>

<h1>Inquirydetails</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Subject</th>
      <th>User</th>
      <th>House</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inquirydetails.each do |inquirydetail| %>
      <tr>
        <td><%= inquirydetail.content %></td>
        <td><%= inquirydetail.subject %></td>
        <td><%= inquirydetail.user %></td>
        <td><%= inquirydetail.house %></td>
        <td><%= link_to 'Show', inquirydetail %></td>
        <td><%= link_to 'Edit', edit_inquirydetail_path(inquirydetail) %></td>
        <td><%= link_to 'Destroy', inquirydetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inquirydetail', new_inquirydetail_path %>
